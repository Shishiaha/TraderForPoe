<Window
	x:Class="TraderForPoe.Windows.StashGridHighlight"
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:Controls="clr-namespace:TraderForPoe.Controls"
	xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
	xmlns:local="clr-namespace:TraderForPoe.Windows"
	xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
	Title="TestGrid"
	AllowsTransparency="True"
	Closing="Window_Closing"
	RenderOptions.BitmapScalingMode="NearestNeighbor"
	ResizeMode="NoResize"
	ShowInTaskbar="False"
	SnapsToDevicePixels="True"
	Topmost="True"
	WindowStyle="None"
	mc:Ignorable="d">
	<Window.Background>
		<SolidColorBrush Opacity="0" />
	</Window.Background>
	<Border BorderBrush="White" BorderThickness="0">
		<Grid>
			<Grid.RowDefinitions>
				<RowDefinition Name="row_1" />
				<RowDefinition Name="row_2" />
				<RowDefinition Name="row_3" />
			</Grid.RowDefinitions>
			<StackPanel
				Name="spnl_Buttons"
				Grid.Row="0"
				Orientation="Horizontal" />
			<StackPanel Grid.Row="1" />
			<Canvas Name="front_canvas" Grid.Row="2" />


		</Grid>
	</Border>
</Window>
